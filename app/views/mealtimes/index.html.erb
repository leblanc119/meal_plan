<div class="page-header">
  <h2>
    Meal Times
  </h2>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Meal Time</th>
      </tr>

      <% @mealtimes.each do |mealtime| %>
      <tr>
        <td><%= mealtime.time %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
