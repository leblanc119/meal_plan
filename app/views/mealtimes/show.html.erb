<div class="page-header">
  <h3>Mealtime #<%= @mealtime.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Time</dt>
      <dd><%= @mealtime.time %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/mealtimes" class="btn btn-primary">
        Back
      </a>
      <a href="/mealtimes/<%= @mealtime.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_mealtime/<%= @mealtime.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
