<div class="page-header">
  <h3>Mealplan #<%= @mealplan.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Mealtime</dt>
      <dd><%= @mealplan.mealtime_id %></dd>

      <dt>Meal</dt>
      <dd><%= @mealplan.meal_id %></dd>

      <dt>Plan</dt>
      <dd><%= @mealplan.plan_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/mealplans" class="btn btn-primary">
        Back
      </a>
      <a href="/mealplans/<%= @mealplan.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_mealplan/<%= @mealplan.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
